{
  "buildCommand": "cd kappish/frontend && npm install && npm run build",
  "outputDirectory": "kappish/frontend/build",
  "installCommand": "cd kappish/frontend && npm install",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 